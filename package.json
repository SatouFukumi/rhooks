{
  "name": "@fukumi/rhooks",
  "private": false,
  "version": "1.0.7",
  "description": "Useful React custom hooks.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "start": "node lib/index.js",
    "watch": "tsbb watch --disable-babel --file-names src/index.ts --emit-type",
    "build": "tsbb build --disable-babel --file-names src/index.ts --emit-type",
    "test": "yarn build && tsbb test",
    "coverage": "yarn build && tsbb test --coverage --bail"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SatouFukumi/rhooks"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "react",
    "hooks",
    "custom hooks",
    "typescript"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.0.25",
    "tsbb": "^3.7.8"
  },
  "dependencies": {
    "@fukumi/libraries": "^1.0.9",
    "react": "^18.2.0",
    "superjson": "^1.11.0"
  }
}
